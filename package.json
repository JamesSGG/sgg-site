{
  "name": "sgg-site",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^8.1.4",
    "npm": "^5.0.3",
    "yarn": "^0.27.5"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --ext .js,.jsx,.graphql src scripts config",
    "graphql:schema": "apollo-codegen introspect-schema http://localhost:8880/graphql --output schema.json",
    "graphql:types": "apollo-codegen generate src/**/*.graphql --schema schema.json --addTypename --target flow --output src/types/graphql.js",
    "graphql": "npm run -s graphql:schema && npm run -s graphql:types"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8880",
      "pathRewrite": {
        "^/api": "/"
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^(?!.*\\.(js|jsx|css|scss|json|gql|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "core-decorators": "^0.19.0",
    "dotenv": "^4.0.0",
    "fs-extra": "^3.0.1",
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "object-assign": "^4.1.1",
    "promise": "^8.0.0",
    "react": "^15.6.1",
    "react-addons-perf": "^15.4.2",
    "react-apollo": "^1.4.3",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.8",
    "react-flip-move": "^2.9.14",
    "react-helmet": "^5.1.3",
    "react-loadable": "^4.0.3",
    "react-perf-tool": "^0.2.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "recompose": "^0.23.5",
    "redux": "^3.7.2",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.70.0",
    "subscriptions-transport-ws": "^0.8.1",
    "universal-cookie": "^2.0.8",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "apollo-codegen": "^0.15.2",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-graphql": "^1.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.49.1",
    "flow-typed": "^2.1.5",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "react-dev-utils": "^3.0.1",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "^0.11.3",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.1.0",
    "workbox-webpack-plugin": "^1.0.1"
  }
}
