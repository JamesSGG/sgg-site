{
  "name": "sgg-site",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^8.4.0",
    "npm": "^5.3.0",
    "yarn": "^1.0.0"
  },
  "scripts": {
    "start": "node scripts/start",
    "start:analyze": "WEBPACK_ANALYZE=true npm run --silent start",
    "build": "node scripts/build",
    "build:analyze": "WEBPACK_ANALYZE=true npm run --silent build",
    "test": "node scripts/test --env=jsdom",
    "lint": "eslint --ext .js,.jsx,.graphql src scripts config",
    "graphql:schema": "apollo-codegen introspect-schema http://localhost:8880/graphql --output schema.json",
    "graphql:types": "apollo-codegen generate 'src/**/*.graphql' --schema schema.json --addTypename --target flow --output src/types/graphql.js",
    "graphql": "npm run -s graphql:schema && npm run -s graphql:types"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8880",
      "pathRewrite": {
        "^/api": "/"
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^(?!.*\\.(js|jsx|css|scss|json|gql|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "dependencies": {
    "add-graphql-subscriptions": "^1.0.1",
    "apollo-client": "^1.9.3",
    "chalk": "^2.1.0",
    "classnames": "^2.2.5",
    "common-tags": "^1.4.0",
    "core-decorators": "^0.20.0",
    "dotenv": "^4.0.0",
    "fs-extra": "^4.0.1",
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.14",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^2.0.1",
    "react-flip-move": "^2.9.14",
    "react-helmet": "^5.1.3",
    "react-loadable": "^4.0.4",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.0.3",
    "redux-localstorage": "^0.4.1",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.74.1",
    "subscriptions-transport-ws": "^0.9.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "apollo-codegen": "^0.17.0-alpha.6",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-css-modules": "^3.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-optional-catch-binding": "^7.0.0-alpha.16",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-optional-catch-binding": "^7.0.0-alpha.16",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-graphql": "^1.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "flow-bin": "^0.56.0",
    "flow-typed": "^2.1.5",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.3.1",
    "jest": "^21.1.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react-dev-utils": "^4.0.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "^1.3.1",
    "webpack-merge": "^4.1.0",
    "workbox-webpack-plugin": "^2.0.2-rc1"
  }
}
